module priority_decoder_3_to_8_dataflow (
    input a, b, c,         
    output y0, y1, y2, y3, y4, y5, y6, y7
);
    assign y7 = c;                       
    assign y3 = (~c & b);                
    assign y1 = (~c & ~b & a);          
    assign y0 = 0;
    assign y2 = 0;
    assign y4 = 0;
    assign y5 = 0;
    assign y6 = 0;

endmodule
